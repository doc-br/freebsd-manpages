#. type: It
#: /usr/src/contrib/mandoc/roff.7:2113
#, no-wrap
msgid "Ic \\eM[ Ns Ar name Ns Ic \\&]"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/mandoc/roff.7:2120
msgid ""
"Set fill (background) color (groff extension); ignored by E<.Xr mandoc 1>.  "
"For short names, there are variants E<.Ic \\eM Ns Ar c> and E<.Ic \\eM( Ns "
"Ar cc>."
msgstr ""

#. type: It
#: /usr/src/contrib/mandoc/roff.7:2120
#, no-wrap
msgid "Ic \\em[ Ns Ar name Ns Ic \\&]"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/mandoc/roff.7:2127
msgid ""
"Set glyph drawing color (groff extension); ignored by E<.Xr mandoc 1>.  For "
"short names, there are variants E<.Ic \\em Ns Ar c> and E<.Ic \\em( Ns Ar "
"cc>."
msgstr ""

#. type: It
#: /usr/src/contrib/mandoc/roff.7:2127
#, no-wrap
msgid "Ic \\eN\\(aq Ns Ar number Ns Ic \\(aq"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/mandoc/roff.7:2131
