msgid "Leader character; ignored by E<.Xr mandoc 1>."
msgstr ""

#. type: It
#: /usr/src/contrib/mandoc/roff.7:2025
#, no-wrap
msgid "Ic \\eB\\(aq Ns Ar string Ns Ic \\(aq"
msgstr ""

