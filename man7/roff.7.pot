#. type: It
#: /usr/src/contrib/mandoc/roff.7:2059
#, no-wrap
msgid "Ic \\ee"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/mandoc/roff.7:2061
msgid "Backslash special character."
msgstr ""

#. type: It
#: /usr/src/contrib/mandoc/roff.7:2061
#, no-wrap
msgid "Ic \\eF[ Ns Ar name Ns Ic \\&]"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/mandoc/roff.7:2068
msgid ""
"Switch font family (groff extension); ignored by E<.Xr mandoc 1>.  For short "
"names, there are variants E<.Ic \\eF Ns Ar c> and E<.Ic \\eF( Ns Ar cc>."
msgstr ""

#. type: It
#: /usr/src/contrib/mandoc/roff.7:2068
#, no-wrap
msgid "Ic \\ef[ Ns Ar name Ns Ic \\&]"
msgstr ""

