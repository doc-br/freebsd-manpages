msgstr ""

#. type: Plain text
#: /usr/src/contrib/mandoc/roff.7:1831
msgid "subtraction"
msgstr ""

#. type: It
#: /usr/src/contrib/mandoc/roff.7:1831
#, no-wrap
msgid "Ic *"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/mandoc/roff.7:1833
msgid "multiplication"
msgstr ""

#. type: It
#: /usr/src/contrib/mandoc/roff.7:1833
#, no-wrap
msgid "Ic /"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/mandoc/roff.7:1835
msgid "division"
