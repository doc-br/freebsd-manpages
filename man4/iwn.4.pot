#. type: Sh
#: /usr/src/share/man/man4/iwn.4:136
#, no-wrap
msgid "EXAMPLES"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:138
msgid "Join an existing BSS network (i.e., connect to an access point):"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:141
#, no-wrap
msgid ""
"ifconfig wlan create wlandev iwn0 inet 192.168.0.20 \\e\n"
"    netmask 0xffffff00\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:145
msgid "Join a specific BSS network with network name E<.Dq Li my_net>:"
msgstr ""

#. type: Dl
#: /usr/src/share/man/man4/iwn.4:146
#, no-wrap
msgid "ifconfig wlan create wlandev iwn0 ssid my_net up"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:149
msgid "Join a specific BSS network with 64-bit WEP encryption:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:152
#, no-wrap
msgid ""
"ifconfig wlan create wlandev iwn0 ssid my_net \\e\n"
"\twepmode on wepkey 0x1234567890 weptxkey 1 up\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:155
msgid "Join a specific BSS network with 128-bit WEP encryption:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:158
#, no-wrap
msgid ""
"ifconfig wlan create wlandev iwn0 wlanmode adhoc ssid my_net \\e\n"
"    wepmode on wepkey 0x01020304050607080910111213 weptxkey 1\n"
msgstr ""

