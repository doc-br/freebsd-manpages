#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:38
msgid ""
"To compile this driver into the kernel, include the following lines in your "
"kernel configuration file:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:43
#, no-wrap
msgid ""
"E<.Cd \"device iwn\">\n"
"E<.Cd \"device pci\">\n"
"E<.Cd \"device wlan\">\n"
"E<.Cd \"device firmware\">\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:47
msgid "You also need to select a firmware for your device.  Choose one from:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:61
#, no-wrap
msgid ""
"E<.Cd \"device iwn1000fw\">\n"
"E<.Cd \"device iwn100fw\">\n"
"E<.Cd \"device iwn105fw\">\n"
"E<.Cd \"device iwn135fw\">\n"
"E<.Cd \"device iwn2000fw\">\n"
"E<.Cd \"device iwn2030fw\">\n"
"E<.Cd \"device iwn4965fw\">\n"
"E<.Cd \"device iwn5000fw\">\n"
"E<.Cd \"device iwn5150fw\">\n"
"E<.Cd \"device iwn6000fw\">\n"
"E<.Cd \"device iwn6000g2afw\">\n"
"E<.Cd \"device iwn6000g2bfw\">\n"
"E<.Cd \"device iwn6050fw\">\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:64
msgid "Or you can use"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:66
#, no-wrap
msgid "E<.Cd \"device iwnfw\">\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:69
msgid "to include them all."
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:73
msgid ""
"Alternatively, to load the driver as a module at boot time, place the "
"following lines in E<.Xr loader.conf 5>:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/iwn.4:88
#, no-wrap
msgid ""
"if_iwn_load=\"YES\"\n"
"iwn1000fw_load=\"YES\"\n"
"iwn100fw_load=\"YES\"\n"
"iwn105fw_load=\"YES\"\n"
"iwn135fw_load=\"YES\"\n"
"iwn2000fw_load=\"YES\"\n"
"iwn2030fw_load=\"YES\"\n"
"iwn4965fw_load=\"YES\"\n"
"iwn5000fw_load=\"YES\"\n"
"iwn5150fw_load=\"YES\"\n"
"iwn6000fw_load=\"YES\"\n"
"iwn6000g2afw_load=\"YES\"\n"
"iwn6000g2bfw_load=\"YES\"\n"
"iwn6050fw_load=\"YES\"\n"
msgstr ""

