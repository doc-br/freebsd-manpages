#, no-wrap
msgid "EXAMPLES"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/zyd.4:132
msgid ""
"The following example configures zyd0 to join any BSS network using WEP key "
"E<.Dq 0x1deadbeef1>, channel 11:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/zyd.4:136
#, no-wrap
msgid ""
"ifconfig wlan create wlandev zyd0 channel 11 \\e\n"
"    wepmode on wepkey 0x1deadbeef1 weptxkey 1 \\e\n"
"    inet 192.168.1.1 netmask 255.255.255.0\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/zyd.4:140
msgid "Join an existing BSS network, E<.Dq my_net>:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/zyd.4:143
#, no-wrap
msgid ""
"ifconfig wlan create wlandev zyd0 192.168.0.2 \\e\n"
"    netmask 0xffffff00 ssid my_net\n"
msgstr ""

