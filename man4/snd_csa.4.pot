#. type: Plain text
#: /usr/src/share/man/man4/snd_csa.4:37
msgid ""
"To compile this driver into the kernel, place the following lines in your "
"kernel configuration file:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_csa.4:40
#, no-wrap
msgid ""
"E<.Cd \"device sound\">\n"
"E<.Cd \"device snd_csa\">\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_csa.4:45
msgid ""
"Alternatively, to load the driver as a module at boot time, place the "
"following line in E<.Xr loader.conf 5>:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_csa.4:47
#, no-wrap
msgid "snd_csa_load=\"YES\"\n"
msgstr ""

