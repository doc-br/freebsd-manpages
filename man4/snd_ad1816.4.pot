msgid "Analog Devices AD1816 ISA bridge device driver"
msgstr ""

#. type: Sh
#: /usr/src/share/man/man4/snd_ad1816.4:33
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_ad1816.4:36
msgid ""
"To compile this driver into the kernel, place the following lines in your "
"kernel configuration file:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_ad1816.4:39
#, no-wrap
msgid ""
"E<.Cd \"device sound\">\n"
"E<.Cd \"device snd_ad1816\">\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_ad1816.4:44
msgid ""
"Alternatively, to load the driver as a module at boot time, place the "
"following line in E<.Xr loader.conf 5>:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_ad1816.4:46
#, no-wrap
msgid "snd_ad1816_load=\"YES\"\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_ad1816.4:50
msgid "Non-PnP cards require the following lines in E<.Xr device.hints 5>:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_ad1816.4:55
#, no-wrap
msgid ""
"hint.pcm.0.at=\"isa\"\n"
"hint.pcm.0.irq=\"10\"\n"
"hint.pcm.0.drq=\"1\"\n"
"hint.pcm.0.flags=\"0x0\"\n"
msgstr ""

