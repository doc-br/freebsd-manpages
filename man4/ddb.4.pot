#. type: Plain text
#: /usr/src/share/man/man4/ddb.4:233
msgid "look at by bytes (8 bits)"
msgstr ""

#. type: It
#: /usr/src/share/man/man4/ddb.4:233
#, no-wrap
msgid "Cm h"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/ddb.4:235
msgid "look at by half words (16 bits)"
msgstr ""

#. type: It
#: /usr/src/share/man/man4/ddb.4:235
#, no-wrap
msgid "Cm l"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/ddb.4:237
msgid "look at by long words (32 bits)"
msgstr ""

#. type: It
#: /usr/src/share/man/man4/ddb.4:237
#, no-wrap
msgid "Cm g"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/ddb.4:239
msgid "look at by quad words (64 bits)"
msgstr ""

#. type: It
#: /usr/src/share/man/man4/ddb.4:239
#, no-wrap
msgid "Cm a"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/ddb.4:241
msgid "print the location being displayed"
