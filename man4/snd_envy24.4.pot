msgstr ""

#. type: Nd
#: /usr/src/share/man/man4/snd_envy24.4:32
#, no-wrap
msgid "VIA Envy24 and compatible bridge device driver"
msgstr ""

#. type: Sh
#: /usr/src/share/man/man4/snd_envy24.4:33
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_envy24.4:36
msgid ""
"To compile this driver into the kernel, place the following lines in your "
"kernel configuration file:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_envy24.4:40
#, no-wrap
msgid ""
"E<.Cd \"device sound\">\n"
"E<.Cd \"device snd_envy24\">\n"
"E<.Cd \"device snd_spicds\">\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_envy24.4:45
msgid ""
"Alternatively, to load the driver as a module at boot time, place the "
"following line in E<.Xr loader.conf 5>:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_envy24.4:47
#, no-wrap
msgid "snd_envy24_load=\"YES\"\n"
msgstr ""

#. type: Sh
#: /usr/src/share/man/man4/snd_envy24.4:48
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_envy24.4:55
msgid ""
"The E<.Nm> bridge driver allows the generic audio driver E<.Xr sound 4> to "
"attach to VIA Envy24 (ICE1724 or VT1724 chipset) and compatible audio "
"devices."
msgstr ""

#. type: Sh
#: /usr/src/share/man/man4/snd_envy24.4:55
#, no-wrap
msgid "HARDWARE"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_envy24.4:59
msgid "The E<.Nm> driver supports the following audio devices:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_envy24.4:63
msgid "M-Audio Audiophile 2496"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_envy24.4:65
msgid "M-Audio Delta Dio 2496"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_envy24.4:67
msgid "Terratec DMX 6fire"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/snd_envy24.4:70
msgid ""
"Only analog playback is supported.  Recording and other features of these "
"cards are not supported."
msgstr ""

