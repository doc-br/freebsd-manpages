#. type: Plain text
#: /usr/src/share/man/man4/bfe.4:53
msgid ""
"The E<.Nm> driver provides support for Broadcom BCM4401 based Fast Ethernet "
"adapters."
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/bfe.4:57
msgid "The E<.Nm> driver supports the following media types:"
msgstr ""

#. type: It
#: /usr/src/share/man/man4/bfe.4:58
#, no-wrap
msgid "Cm autoselect"
