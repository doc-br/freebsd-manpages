#. type: Plain text
#: /usr/src/share/man/man4/smsc.4:53
#, no-wrap
msgid ""
"E<.Cd \"device uhci\">\n"
"E<.Cd \"device ohci\">\n"
"E<.Cd \"device usb\">\n"
"E<.Cd \"device miibus\">\n"
"E<.Cd \"device uether\">\n"
"E<.Cd \"device smsc\">\n"
msgstr ""

