#. type: Plain text
#: /usr/src/share/man/man4/ural.4:108
msgid "Join an existing BSS network (i.e., connect to an access point):"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/ural.4:111
#, no-wrap
msgid ""
"ifconfig wlan create wlandev ural0 inet 192.168.0.20 \\e\n"
"    netmask 0xffffff00\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/ural.4:115
msgid "Join a specific BSS network with network name E<.Dq Li my_net>:"
msgstr ""

#. type: Dl
#: /usr/src/share/man/man4/ural.4:116
#, no-wrap
msgid "ifconfig wlan create wlandev ural0 ssid my_net up"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/ural.4:119
msgid "Join a specific BSS network with 64-bit WEP encryption:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/ural.4:122
#, no-wrap
msgid ""
"ifconfig wlan create wlandev ural0 ssid my_net \\e\n"
"\twepmode on wepkey 0x1234567890 weptxkey 1 up\n"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/ural.4:125
msgid "Join a specific BSS network with 128-bit WEP encryption:"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man4/ural.4:128
#, no-wrap
msgid ""
"ifconfig wlan create wlandev ural0 wlanmode adhoc ssid my_net \\e\n"
"    wepmode on wepkey 0x01020304050607080910111213 weptxkey 1\n"
msgstr ""

