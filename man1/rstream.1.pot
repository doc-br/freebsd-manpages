#, no-wrap
msgid "RSTREAM"
msgstr ""

#. type: TH
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:2
#, no-wrap
msgid "2011-11-16"
msgstr ""

#. type: TH
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:2 /usr/src/contrib/ofed/librdmacm/man/rstream.1:2
#, no-wrap
msgid "librdmacm"
msgstr ""

#. type: SH
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:5
msgid "rstream - streaming over RDMA ping-pong test."
msgstr ""

#. type: SH
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:11
#, no-wrap
msgid ""
"I<rstream> [-s server_address] [-b bind_address] [-f address_format]\n"
"\t\t\t[-B buffer_size] [-I iterations] [-C transfer_count]\n"
"\t\t\t[-S transfer_size] [-p server_port] [-T test_option]\n"
msgstr ""

#. type: SH
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:12
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:15
msgid ""
"Uses the streaming over RDMA protocol (rsocket) to connect and exchange data "
"between a client and server application."
msgstr ""

#. type: SH
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:15
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: TP
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:16
#, no-wrap
msgid "-s server_address"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:21
msgid ""
"The network name or IP address of the server system listening for "
"connections.  The used name or address must route over an RDMA device.  This "
"option must be specified by the client."
msgstr ""

#. type: TP
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:21
#, no-wrap
msgid "-b bind_address"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:24
msgid "The local network address to bind to."
msgstr ""

#. type: TP
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:24
#, no-wrap
msgid "-f address_format"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:27
msgid "Supported address formats are ip, ipv6, gid, or name."
msgstr ""

#. type: TP
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:27
#, no-wrap
msgid "-B buffer_size"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:30
msgid "Indicates the size of the send and receive network buffers."
msgstr ""

#. type: TP
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:30
#, no-wrap
msgid "-I iterations"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:34
msgid ""
"The number of times that the specified number of messages will be exchanged "
"between the client and server.  (default 1000)"
msgstr ""

#. type: TP
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:34
#, no-wrap
msgid "-C transfer_count"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:38
msgid ""
"The number of messages to transfer from the client to the server and back "
"again on each iteration.  (default 1000)"
msgstr ""

#. type: TP
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:38
#, no-wrap
msgid "-S transfer_size"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:42
msgid ""
"The size of each send transfer, in bytes.  (default 1000)  If 'all' is "
"specified, rstream will run a series of tests of various sizes."
msgstr ""

#. type: TP
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:42
#, no-wrap
msgid "-p server_port"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:45
msgid "The server's port number."
msgstr ""

#. type: TP
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:45
#, no-wrap
msgid "-T test_option"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:48
msgid "Specifies test parameters.  Available options are:"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:50
msgid "s | socket - uses standard socket calls to transfer data"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:52
msgid "a | async - uses asynchronous operation (e.g. select / poll)"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:54
msgid "b | blocking - uses blocking calls"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:56
msgid "f | fork - fork server processing (forces -T s option)"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:58
msgid "n | nonblocking - uses non-blocking calls"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:60
msgid "r | resolve - use rdma cm to resolve address"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:62
msgid "v | verify - verifies data transfers"
msgstr ""

#. type: SH
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:62
#, no-wrap
msgid "NOTES"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:69
msgid ""
"Basic usage is to start rstream on a server system, then run rstream -s "
"server_name on a client system.  By default, rstream will run a series of "
"latency and bandwidth performance tests.  Specifying a different iterations, "
"transfer_count, or transfer_size will run a user customized test using "
"default values where none have been specified."
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:73
msgid ""
"Because this test maps RDMA resources to userspace, users must ensure that "
"they have available system resources and permissions.  See the libibverbs "
"README file for additional details."
msgstr ""

#. type: SH
#: /usr/src/contrib/ofed/librdmacm/man/rstream.1:73
#, no-wrap
msgid "SEE ALSO"
msgstr ""

