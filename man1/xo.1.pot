msgstr ""

#. type: It
#: /usr/src/contrib/libxo/xo/xo.1:63
#, no-wrap
msgid "Ic -T | Ic --text"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/libxo/xo/xo.1:65
msgid "Generate text output (the default style)"
msgstr ""

#. type: It
#: /usr/src/contrib/libxo/xo/xo.1:65
#, no-wrap
msgid "Ic --top-warp"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/libxo/xo/xo.1:68
msgid ""
"Indicates the entire object should be placed inside a top-level object "
"wrapper, specifically when generating JSON output."
msgstr ""

#. type: It
#: /usr/src/contrib/libxo/xo/xo.1:68
#, no-wrap
msgid "Ic --version"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/libxo/xo/xo.1:70
msgid "Display version information"
msgstr ""

#. type: It
#: /usr/src/contrib/libxo/xo/xo.1:70
#, no-wrap
