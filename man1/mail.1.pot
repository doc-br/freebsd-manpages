#, no-wrap
msgid "Pa /usr/share/misc/mail.rc"
msgstr ""

#. type: It
#: /usr/src/usr.bin/mail/mail.1:1234
#, no-wrap
msgid "Pa /usr/local/etc/mail.rc"
msgstr ""

#. type: It
#: /usr/src/usr.bin/mail/mail.1:1235
#, no-wrap
msgid "Pa /etc/mail.rc"
msgstr ""

