#, no-wrap
msgid "Pa ~/.mailrc"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/mail/mail.1:1228
msgid ""
"File giving initial E<.Nm> commands.  This can be overridden by setting the "
"E<.Ev MAILRC> environment variable."
msgstr ""

#. type: It
#: /usr/src/usr.bin/mail/mail.1:1228
#, no-wrap
msgid "Pa /tmp/R*"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/mail/mail.1:1230
msgid "Temporary files."
msgstr ""

#. type: It
#: /usr/src/usr.bin/mail/mail.1:1230
#, no-wrap
msgid "Pa /usr/share/misc/mail.*help"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/mail/mail.1:1232
msgid "Help files."
msgstr ""

