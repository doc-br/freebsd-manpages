#. type: Sh
#: /usr/src/crypto/heimdal/appl/rcp/rcp.1:9
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/crypto/heimdal/appl/rcp/rcp.1:18
msgid ""
"E<.Nm rcp> E<.Op Fl 45FKpxz> E<.Op Fl P Ar port> E<.Ar file1 file2> E<.Nm "
"rcp> E<.Op Fl 45FKprxz> E<.Op Fl P Ar port> E<.Ar file... directory>"
msgstr ""

#. type: Sh
#: /usr/src/crypto/heimdal/appl/rcp/rcp.1:18
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: /usr/src/crypto/heimdal/appl/rcp/rcp.1:24
msgid ""
"E<.Nm rcp> copies files between machines. Each file argument is either a "
"remote file name of the form E<.Dq rname@rhost:path> or a local file "
"(containing no colon or with a slash before the first colon)."
msgstr ""

