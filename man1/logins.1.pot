#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:45
msgid "The E<.Nm> utility displays information about user and system accounts."
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:47
msgid "The following options are available:"
msgstr ""

#. type: It
#: /usr/src/usr.bin/logins/logins.1:48
#, no-wrap
msgid "Fl a"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:51
msgid ""
"Display information about the password change and account expiration times "
"for each account."
msgstr ""

#. type: It
#: /usr/src/usr.bin/logins/logins.1:51
#, no-wrap
msgid "Fl d"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:53
msgid "Select accounts with duplicate UIDs."
msgstr ""

#. type: It
#: /usr/src/usr.bin/logins/logins.1:53
#, no-wrap
msgid "Fl g Ar groups"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:57
msgid ""
"Select accounts that are members of the specified groups.  If multiple group "
"names are specified, they must be separated with commas."
msgstr ""

#. type: It
#: /usr/src/usr.bin/logins/logins.1:57
#, no-wrap
msgid "Fl l Ar logins"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:60
msgid ""
"Select accounts matching the specified login names.  If multiple names are "
"specified, they must be separated with commas."
msgstr ""

#. type: It
#: /usr/src/usr.bin/logins/logins.1:60
#, no-wrap
msgid "Fl m"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:62
msgid "Show information about secondary groups."
msgstr ""

#. type: It
#: /usr/src/usr.bin/logins/logins.1:62
#, no-wrap
msgid "Fl o"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:65
msgid ""
"Display the information for each account on a single line of colon-separated "
"fields."
msgstr ""

#. type: It
#: /usr/src/usr.bin/logins/logins.1:65
#, no-wrap
msgid "Fl p"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:67
msgid "Select accounts with no password."
msgstr ""

#. type: It
#: /usr/src/usr.bin/logins/logins.1:67
#, no-wrap
msgid "Fl s"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:72
msgid ""
"Select system accounts.  These are currently defined as accounts with UIDs "
"below 1000, plus the E<.Dq Li nobody> account (UID 65534)."
msgstr ""

#. type: It
#: /usr/src/usr.bin/logins/logins.1:72
#, no-wrap
msgid "Fl t"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:74
msgid "Sort selected accounts by name rather than by UID."
msgstr ""

#. type: It
#: /usr/src/usr.bin/logins/logins.1:74
#, no-wrap
msgid "Fl u"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:80
msgid ""
"Select user accounts.  These are currently defined as accounts with UIDs "
"above 1000, except the E<.Dq Li nobody> account (UID 65534)."
msgstr ""

#. type: It
#: /usr/src/usr.bin/logins/logins.1:80
#, no-wrap
msgid "Fl x"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:82
msgid "Display information about each account's home directory and shell."
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:86
msgid ""
"If multiple selection options are specified, all accounts matching any of "
"the selection criteria will be displayed."
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/logins/logins.1:88
msgid "If no selection options are specified, all accounts will be displayed."
msgstr ""

