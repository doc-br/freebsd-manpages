#, no-wrap
msgid "Li \\&-"
msgstr ""

#. type: Plain text
#: /usr/src/usr.bin/who/who.1:78
msgid "User is not accepting messages."
msgstr ""

#. type: It
#: /usr/src/usr.bin/who/who.1:78
#, no-wrap
msgid "Li \\&?"
msgstr ""

