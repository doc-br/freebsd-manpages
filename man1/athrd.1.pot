#. type: Nd
#: /usr/src/tools/tools/ath/athrd/athrd.1:36
#, no-wrap
msgid "list channels and transmit power for a country/regulatory domain"
msgstr ""

#. type: Sh
#: /usr/src/tools/tools/ath/athrd/athrd.1:37
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/tools/tools/ath/athrd/athrd.1:44
msgid ""
"E<.Nm> E<.Op Fl aedlpcfr4ABGT> E<.Op Fl m Ar mode> E<.Bk> E<.Op Ar country> "
"E<.Ek>"
msgstr ""

