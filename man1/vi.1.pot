#. type: Plain text
#: /usr/src/contrib/nvi/docs/man/vi.1:236
msgid "Move the cursor down one line."
msgstr ""

#. type: It
#: /usr/src/contrib/nvi/docs/man/vi.1:236
#, no-wrap
msgid "Cm k"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/nvi/docs/man/vi.1:238
msgid "Move the cursor up one line."
