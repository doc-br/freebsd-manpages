msgid "ldns-config - show compiler and linker flags for ldns usage."
msgstr ""

#. type: SH
#: /usr/src/contrib/ldns/packaging/ldns-config.1:4
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ldns/packaging/ldns-config.1:9
msgid "B<ldns-config> [ I<OPTIONS> ]"
msgstr ""

#. type: SH
#: /usr/src/contrib/ldns/packaging/ldns-config.1:10
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ldns/packaging/ldns-config.1:15
msgid ""
"When writing programs using ldns, you have to tell the compiler where to "
"look for include files and what libraries from which location to link "
"to. B<ldns-config> can be used to find out what flags to use with the C "
"compiler and the linker."
msgstr ""

#. type: SH
#: /usr/src/contrib/ldns/packaging/ldns-config.1:16
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: TP
#: /usr/src/contrib/ldns/packaging/ldns-config.1:17
#, no-wrap
msgid "B<--cflags>"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ldns/packaging/ldns-config.1:20
msgid "Show the C compiler flags needed to compile with ldns"
msgstr ""

#. type: TP
#: /usr/src/contrib/ldns/packaging/ldns-config.1:21
#, no-wrap
msgid "B<--libs>"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ldns/packaging/ldns-config.1:24
msgid "Show the flags to be used to link with ldns"
msgstr ""

#. type: TP
#: /usr/src/contrib/ldns/packaging/ldns-config.1:25
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ldns/packaging/ldns-config.1:28
msgid "Shows the ldns version of the installed ldns library"
msgstr ""

#. type: TP
#: /usr/src/contrib/ldns/packaging/ldns-config.1:29
#, no-wrap
msgid "B<--libversion>"
msgstr ""

#. type: Plain text
