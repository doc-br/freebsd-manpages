#. type: Plain text
#: /usr/src/sys/contrib/zstd/programs/zstdgrep.1:6
msgid "B<zstdgrep> - print lines matching a pattern in zstandard-compressed files"
msgstr ""

#. type: SH
#: /usr/src/sys/contrib/zstd/programs/zstdgrep.1:7
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/sys/contrib/zstd/programs/zstdgrep.1:9
msgid "B<zstdgrep> [I<grep-flags>] [--] I<pattern> [I<files> ...]"
msgstr ""

#. type: SH
#: /usr/src/sys/contrib/zstd/programs/zstdgrep.1:10
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: /usr/src/sys/contrib/zstd/programs/zstdgrep.1:12
msgid ""
"B<zstdgrep> runs B<grep (1)> on files or stdin, if no files argument is "
"given, after decompressing them with B<zstdcat (1)>."
msgstr ""

#. type: Plain text
#: /usr/src/sys/contrib/zstd/programs/zstdgrep.1:15
msgid ""
"The grep-flags and pattern arguments are passed on to B<grep (1)>. If an "
"B<-e> flag is found in the B<grep-flags>, B<zstdgrep> will not look for a "
"pattern argument."
msgstr ""

#. type: SH
#: /usr/src/sys/contrib/zstd/programs/zstdgrep.1:16
#, no-wrap
msgid "EXIT STATUS"
msgstr ""

#. type: Plain text
#: /usr/src/sys/contrib/zstd/programs/zstdgrep.1:18
msgid ""
"In case of missing arguments or missing pattern, 1 will be returned, "
"otherwise 0."
msgstr ""

