#: /usr/src/bin/sh/sh.1:407
#, no-wrap
msgid "Li E<lt> Ta Li E<gt> Ta Li E<lt>E<lt> Ta Li E<gt>E<gt> Ta Li E<lt>E<gt>"
msgstr ""

#. type: It
#: /usr/src/bin/sh/sh.1:408
#, no-wrap
msgid "Li E<lt>& Ta Li E<gt>& Ta Li E<lt>E<lt>- Ta Li E<gt>| Ta \\&"
msgstr ""

#. type: Plain text
#: /usr/src/bin/sh/sh.1:418
msgid ""
"The character E<.Ql #> introduces a comment if used at the beginning of a "
"word.  The word starting with E<.Ql #> and the rest of the line are ignored."
msgstr ""

#. type: Plain text
#: /usr/src/bin/sh/sh.1:422
msgid ""
"ASCII E<.Dv NUL> characters (character code 0) are not allowed in shell "
"input."
msgstr ""

