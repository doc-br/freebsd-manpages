msgid "The maximal size of memory that can be locked by a process, in kilobytes."
msgstr ""

#. type: It
#: /usr/src/bin/sh/sh.1:2691
#, no-wrap
msgid "Fl m Ar memoryuse"
msgstr ""

#. type: Plain text
#: /usr/src/bin/sh/sh.1:2693
msgid "The maximal resident set size of a process, in kilobytes."
msgstr ""

#. type: It
#: /usr/src/bin/sh/sh.1:2693
