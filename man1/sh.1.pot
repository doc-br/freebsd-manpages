#. type: Plain text
#: /usr/src/bin/sh/sh.1:2695
msgid "The maximal number of descriptors that could be opened by a process."
msgstr ""

#. type: It
#: /usr/src/bin/sh/sh.1:2695
#, no-wrap
msgid "Fl o Ar umtxp"
msgstr ""

