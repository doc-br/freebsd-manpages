msgstr ""

#. type: Sh
#: /usr/src/bin/pwait/pwait.1:76
#, no-wrap
msgid "EXIT STATUS"
msgstr ""

#. type: Plain text
#: /usr/src/bin/pwait/pwait.1:80
msgid "The E<.Nm> utility exits 0 on success, and E<gt>0 if an error occurs."
msgstr ""

#. type: Plain text
#: /usr/src/bin/pwait/pwait.1:84
