#, no-wrap
msgid "IPSCAN"
msgstr ""

#. type: SH
#: /usr/src/contrib/ipfilter/man/ipscan.8:4
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ipfilter/man/ipscan.8:6
msgid "ipscan - user interface to the IPFilter content scanning"
msgstr ""

#. type: SH
#: /usr/src/contrib/ipfilter/man/ipscan.8:6
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ipfilter/man/ipscan.8:13
msgid "B<ipscan> [ B<-dlnrsv> ] [ ] B<-f E<lt>>I<filename>B<E<gt>>"
msgstr ""

#. type: SH
#: /usr/src/contrib/ipfilter/man/ipscan.8:13
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ipfilter/man/ipscan.8:18
msgid ""
"B<ipscan> opens the filename given (treating \"-\" as stdin) and parses the "
"file to build up a content scanning configuration to load into the kernel.  "
"Currently only the first 16 bytes of a connection can be compared."
msgstr ""

#. type: SH
#: /usr/src/contrib/ipfilter/man/ipscan.8:18
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: TP
#: /usr/src/contrib/ipfilter/man/ipscan.8:19
#, no-wrap
msgid "B<-d>"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ipfilter/man/ipscan.8:22
msgid "Toggle debugging of processing the configuration file."
msgstr ""

#. type: TP
#: /usr/src/contrib/ipfilter/man/ipscan.8:22
#, no-wrap
msgid "B<-l>"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ipfilter/man/ipscan.8:25
msgid "Show the list of currently configured content scanning entries."
msgstr ""

#. type: TP
#: /usr/src/contrib/ipfilter/man/ipscan.8:25
#, no-wrap
msgid "B<-n>"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ipfilter/man/ipscan.8:29
msgid ""
"This flag (no-change) prevents B<ipscan> from actually making any ioctl "
"calls or doing anything which would alter the currently running kernel."
msgstr ""

#. type: TP
#: /usr/src/contrib/ipfilter/man/ipscan.8:29
#, no-wrap
msgid "B<-r>"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ipfilter/man/ipscan.8:33
msgid ""
"Remove commands from kernel configuration as they are read from the "
"configuration file rather than adding new ones."
msgstr ""

#. type: TP
#: /usr/src/contrib/ipfilter/man/ipscan.8:33
#, no-wrap
msgid "B<-s>"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ipfilter/man/ipscan.8:36
msgid "Retrieve and display content scanning statistics"
msgstr ""

#. type: TP
#: /usr/src/contrib/ipfilter/man/ipscan.8:36
#, no-wrap
msgid "B<-v>"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/ipfilter/man/ipscan.8:39
msgid "Turn verbose mode on."
msgstr ""

