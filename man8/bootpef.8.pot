msgid "BOOTPEF 8"
msgstr ""

#. type: Sh
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:7
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:9
msgid "E<.Nm bootpef>"
msgstr ""

#. type: Nd
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:9
#, no-wrap
msgid "BOOTP Extension File compiler"
msgstr ""

#. type: Sh
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:10
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:18
msgid ""
"E<.Bk -words> E<.Nm> E<.Op Fl c Ar chdir\\-path> E<.Op Fl d Ar "
"debug\\-level> E<.Op Fl f Ar config\\-file> E<.Op Ar client\\-name ...> "
"E<.Ek>"
msgstr ""

#. type: Sh
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:18
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:31
msgid ""
"The E<.Nm> utility builds the E<.Em \"Extension Path\"> files described by "
"E<.%T \"RFC 1497\"> (tag 18).  If any E<.Ar client\\-name> arguments are "
"specified, then E<.Nm> compiles the extension files for only those clients."
msgstr ""

#. type: Sh
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:31
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: It
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:33
#, no-wrap
msgid "Fl c Ar chdir\\-path"
msgstr ""

#. type: Plain text
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:43
msgid ""
"Sets the current directory used by E<.Nm> while creating extension files.  "
"This is useful when the extension file names are specified as relative "
"pathnames, and E<.Nm> needs to use the same current directory as the TFTP "
"server (typically E<.Pa /tftpboot>)."
msgstr ""

#. type: It
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:43
#, no-wrap
msgid "Fl d Ar debug\\-level"
msgstr ""

#. type: Plain text
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:50
msgid ""
"Sets the E<.Ar debug\\-level> variable that controls the amount of debugging "
"messages generated.  For example, E<.Fl d Ar 4> will set the debugging level "
"to 4."
msgstr ""

#. type: It
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:50
#, no-wrap
msgid "Fl f Ar config\\-file"
msgstr ""

#. type: Plain text
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:53
msgid ""
"Set the name of the config file that specifies the option data to be sent to "
"each client."
msgstr ""

#. type: Sh
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:54
#, no-wrap
msgid "SEE ALSO"
msgstr ""

#. type: Plain text
#: /usr/src/libexec/bootpd/tools/bootpef/bootpef.8:64
msgid ""
"E<.Xr bootpd 8>, E<.Xr tftpd 8> E<.Rs> E<.%O RFC951> E<.%T \"BOOTSTRAP "
"PROTOCOL (BOOTP)\"> E<.Re> E<.Rs> E<.%O RFC1497> E<.%T \"BOOTP Vendor "
"Information Extensions\"> E<.Re>"
msgstr ""
