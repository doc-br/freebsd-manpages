#: /usr/src/contrib/amd/amd/sun2amd.8:44
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/amd/sun2amd.8:47
msgid "B<sun2amd> [-hH] [-i infile] [-o outfile]"
msgstr ""

