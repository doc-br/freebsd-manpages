msgstr ""

#. type: Plain text
#: /usr/src/sbin/camcontrol/camcontrol.8:924
msgid ""
"Set the minimum physical link rate for the phy.  This is a numeric "
"argument.  Currently known link rates are:"
msgstr ""

#. type: It
#: /usr/src/sbin/camcontrol/camcontrol.8:925
#, no-wrap
msgid "0x0"
msgstr ""

#. type: Plain text
#: /usr/src/sbin/camcontrol/camcontrol.8:927
msgid "Do not change current value."
msgstr ""

#. type: It
#: /usr/src/sbin/camcontrol/camcontrol.8:927
#, no-wrap
msgid "0x8"
msgstr ""

#. type: Plain text
#: /usr/src/sbin/camcontrol/camcontrol.8:929
msgid "1.5 Gbps"
msgstr ""

#. type: It
#: /usr/src/sbin/camcontrol/camcontrol.8:929
#, no-wrap
msgid "0x9"
msgstr ""

#. type: Plain text
#: /usr/src/sbin/camcontrol/camcontrol.8:931
msgid "3 Gbps"
msgstr ""

#. type: It
#: /usr/src/sbin/camcontrol/camcontrol.8:931
#, no-wrap
msgid "0xa"
msgstr ""

#. type: Plain text
#: /usr/src/sbin/camcontrol/camcontrol.8:933
msgid "6 Gbps"
msgstr ""

#. type: Plain text
#: /usr/src/sbin/camcontrol/camcontrol.8:936
