#. type: TH
#: /usr/src/contrib/amd/scripts/automount2amd.8:38
#, no-wrap
msgid "AUTOMOUNT2AMD"
msgstr ""

#. type: TH
#: /usr/src/contrib/amd/scripts/automount2amd.8:38
#, no-wrap
msgid "24 May 1993"
msgstr ""

#. type: SH
#: /usr/src/contrib/amd/scripts/automount2amd.8:39
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:41
msgid "automount2amd - converts old Sun automount maps to Amd maps"
msgstr ""

#. type: SH
#: /usr/src/contrib/amd/scripts/automount2amd.8:41
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:44
msgid "B<automount2amd> I<auto.map>"
msgstr ""

#. type: SH
#: /usr/src/contrib/amd/scripts/automount2amd.8:44
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:50
msgid ""
"B<automount2amd> is used to convert an old Sun automount maps named "
"I<auto.map> to an Amd map."
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:52
msgid "This perl script will use the following /default entry"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:54
#, no-wrap
msgid "    type:=nfs;opts:=rw,grpid,nosuid,utimeout=600\n"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:58
msgid ""
"If you wish to override that, define the B<$DEFAULTS> environment variable, "
"or modify the script."
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:64
msgid ""
"If you wish to generate Amd maps using the I<hostd> (host domain) Amd map "
"syntax, then define the environment variable B<$DOMAIN> or modify the "
"script."
msgstr ""

#. type: SH
#: /usr/src/contrib/amd/scripts/automount2amd.8:65
#, no-wrap
msgid "EXAMPLE"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:67
msgid "Say you have the Sun automount file auto.foo, with these two lines:"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:70
#, no-wrap
msgid ""
"  home                  earth:/home\n"
"  moon  -ro,intr        server:/proj/images\n"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:72
msgid "Running"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:74
#, no-wrap
msgid "    automount2amd auto.foo E<gt> amd.foo\n"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:78
msgid "will produce the Amd map I<amd.foo> with this content:"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:80
#, no-wrap
msgid "# generated by automount2amd on Sat Aug 14 17:59:32 US/Eastern 1999\n"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:83
#, no-wrap
msgid ""
"/defaults \\e\n"
"  type:=nfs;opts:=rw,grpid,nosuid,utimeout=600\n"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:87
#, no-wrap
msgid "home   host==earth;type:=link;fs:=/home \\  rhost:=earth;rfs:=/home\n"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:92
#, no-wrap
msgid ""
"moon   -addopts:=ro,intr \\  host==server;type:=link;fs:=/proj/images \\  "
"rhost:=server;rfs:=/proj/images\n"
msgstr ""

#. type: SH
#: /usr/src/contrib/amd/scripts/automount2amd.8:93
#, no-wrap
msgid "BUGS"
msgstr ""

#. type: Plain text
#: /usr/src/contrib/amd/scripts/automount2amd.8:96
msgid ""
"I<automount2amd> does not understand newer Sun Automount map syntax, those "
"used by autofs."
msgstr ""

