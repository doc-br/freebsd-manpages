msgstr ""

#. type: It
#: /usr/src/share/man/man5/pf.conf.5:418 /usr/src/share/man/man5/pf.conf.5:612
#, no-wrap
msgid "Ar none"
msgstr ""

#. type: Plain text
#: /usr/src/share/man/man5/pf.conf.5:420
msgid "Disable the ruleset optimizer."
