"realtime-sc    = \"realtime\" sc-spec\n"
"upperlimit-sc  = \"upperlimit\" sc-spec\n"
"sc-spec        = ( bandwidth-spec |\n"
"                 \"(\" bandwidth-spec number bandwidth-spec \")\" )\n"
"include        = \"include\" filename\n"
